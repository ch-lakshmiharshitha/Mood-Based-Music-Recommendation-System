{
  "name": "ai-mood-music-recommender",
  "version": "1.0.0",
  "description": "AI-Powered Mood-Based Music Recommendation System with CSV Dataset",
  "type": "module",
  "scripts": {
    "start-backend": "node backend/server.js",
    "start-frontend": "cd frontend && python -m http.server 3000",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "install-deps": "npm install cors express csv-parser"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "ai",
    "music",
    "recommendation",
    "mood",
    "csv",
    "machine-learning"
  ],
  "author": "Lakshmi Harshitha",
  "license": "MIT"
}